@{
    Layout = "~/Views/Shared/_ToolBar2.cshtml";
}
@RenderSection("styles", required: false)
<h3>功能列</h3>
<div class="row ">
    <div class="col-md-6 col-md-offset-2">
        <a href="@Url.Content("~/Home")" class="btn btn-primary"
           data-toggle="tooltip" data-placement="bottom" title="回EIP首頁">
            <i class="glyphicon glyphicon-home" aria-hidden="true"></i>
        </a>
        <a href="@Url.Action("ItemAlbum","item")" class="btn btn-primary"
           data-toggle="tooltip" data-placement="bottom" title="瀏覽零件相簿">
            <i class="glyphicon glyphicon-eye-open" aria-hidden="true"></i>
        </a>

        <a href="@Url.Action("AlbumIndex","ManageAlbum")" class="btn btn-primary"
           data-toggle="tooltip" data-placement="bottom" title="管理零件相簿">
            <i class="glyphicon glyphicon-cog" aria-hidden="true"></i>
        </a>
        <a href='@Url.Action("UploadImg","ManageAlbum")' class="btn btn-primary" role="button"
           data-toggle="tooltip" data-placement="bottom" title="新增主分類相簿">
            <i class="glyphicon glyphicon-cloud-upload"></i>
        </a>
        <a class="btn btn-primary" data-toggle="tooltip" data-placement="bottom" title="回上一頁"
           href="javascript:void(0);" onclick="history.go(-1);" role="button">
            <i class="glyphicon glyphicon-level-up" aria-hidden="true"></i>
        </a>
    </div>
</div>
<hr /><!--我是分隔線-->
@RenderBody()

<hr /><!--我是分隔線-->
<div class="row ">
    <div class="col-md-6 col-md-offset-2">
        <a href="@Url.Content("~/Home")" class="btn btn-primary"
           data-toggle="tooltip" data-placement="bottom" title="回EIP首頁">
            <i class="glyphicon glyphicon-home" aria-hidden="true"></i>
        </a>
        <a href="@Url.Action("ItemAlbum","item")" class="btn btn-primary"
           data-toggle="tooltip" data-placement="bottom" title="瀏覽零件相簿">
            <i class="glyphicon glyphicon-eye-open" aria-hidden="true"></i>
        </a>
        <a href="@Url.Action("AlbumIndex","ManageAlbum")" class="btn btn-primary"
           data-toggle="tooltip" data-placement="bottom" title="管理零件相簿">
            <i class="glyphicon glyphicon-cog" aria-hidden="true"></i>
        </a>
        <a href='@Url.Action("UploadImg","ManageAlbum")' class="btn btn-primary" role="button"
           data-toggle="tooltip" data-placement="bottom" title="新增相簿">
            <i class="glyphicon glyphicon-cloud-upload"></i> 
        </a>
        <a class="btn btn-primary" data-toggle="tooltip" data-placement="bottom" title="回上一頁"
           href="javascript:void(0);" onclick="history.go(-1);" role="button">
            <i class="glyphicon glyphicon-level-up" aria-hidden="true"></i>
        </a>
    </div>
</div>

@Scripts.Render("~/bundles/jquery223")
@Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/bundles/jqueryval")
@RenderSection("scripts", required: false)